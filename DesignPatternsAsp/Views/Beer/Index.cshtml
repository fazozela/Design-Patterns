@using DesignPatternsAsp.Models.ViewModels
@model IEnumerable<BeerViewModel>

<div class="row">
    <div class="col-12">
        <a class="btn btn-primary" href="~/Beer/Add">
            Nueva Cerveza
        </a>
    </div>
</div>

<div class="row">
    <div class="col-12">
        <table class=" table">
            <tr>
                <th>Nombre</th>
            </tr>
            @foreach (var item in Model)
            {
                <tr>
                    <td>@item.Name.</td>
                </tr>
            }
        </table>
    </div>
</div>